<config>
    <modules>
        <Magavenue_Shopializable>
            <version>0.1.0</version>
        </Magavenue_Shopializable>
    </modules>
    <frontend>
        <routers>
            <shopializable>
                <use>standard</use>
                <args>
                    <module>Magavenue_Shopializable</module>
                    <frontName>shopializable</frontName>
                </args>
            </shopializable>
        </routers>
    </frontend>
    <translate>
       <modules>
           <Magavenue_Shopializable>
               <files>
                   <default>Magavenue_Shopializable.csv</default>
               </files>
           </Magavenue_Shopializable>
       </modules>
    </translate>
    <global>
        <helpers>
            <shopializable>
                <class>Magavenue_Shopializable_Helper</class>
            </shopializable>
        </helpers>
    </global>
    <default>
        <shopializable>
            <shopializable>
                    <boutique_template>default-reduced</boutique_template>
                    <boutique_nbr>9</boutique_nbr>
            </shopializable>
        </shopializable>
    </default>
</config>